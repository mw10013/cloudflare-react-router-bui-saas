{
  "name": "crrbuis",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "build:PRODUCTION": "CLOUDFLARE_ENV=production pnpm build",
    "cf-typegen": "wrangler types",
    "deploy": "pnpm run build && wrangler deploy",
    "deploy:PRODUCTION": "pnpm build:PRODUCTION && wrangler deploy",
    "dev": "react-router dev",
    "postinstall": "npm run cf-typegen",
    "preview": "pnpm run build && vite preview",
    "typecheck": "npm run cf-typegen && react-router typegen && tsc -b",
    "typecheck:test": "tsc -p test/tsconfig.json",
    "lint": "eslint .",
    "test": "vitest run",
    "test:e2e": "pnpm exec playwright test",
    "clean": "pnpm clean:node && pnpm clean:wrangler && pnpm clean:etc",
    "clean:node": "rm -rf node_modules test/*/node_modules",
    "clean:wrangler": "rm -rf .wrangler",
    "clean:etc": "rm -rf .react-router build dist",
    "tail:PRODUCTION": "wrangler tail crrbuis",
    "d1:migrate:list": "wrangler d1 migrations list d1-local --local",
    "d1:migrate:apply": "wrangler d1 migrations apply d1-local --local",
    "d1:migrate:list:int": "wrangler d1 migrations list crrbuis-d1-int --env int --remote",
    "d1:migrate:apply:int": "wrangler d1 migrations apply crrbuis-d1-int --env int --remote",
    "d1:migrate:list:PRODUCTION": "wrangler d1 migrations list crrbuis-d1-production --env production --remote",
    "d1:migrate:apply:PRODUCTION": "wrangler d1 migrations apply crrbuis-d1-production --env production --remote",
    "d1:reset": "node scripts/d1-reset.ts",
    "d1:reset:int": "node scripts/d1-reset.ts --env int",
    "d1:reset:PRODUCTION": "node scripts/d1-reset.ts --env production",
    "stripe:listen": "stripe listen --project-name=crrbuis-int --load-from-webhooks-api --forward-to localhost:5173/",
    "stripe:listen:crrs-int": "stripe listen --project-name=crrs-int --load-from-webhooks-api --forward-to localhost:5173/",
    "script": "node --env-file=.env",
    "refs:shadcn": "rm -rf refs/shadcn && mkdir -p refs/shadcn && curl -L https://github.com/shadcn-ui/ui/archive/refs/tags/shadcn@3.6.2.tar.gz | tar -xz -C refs/shadcn --strip-components=1",
    "refs:tan-form": "rm -rf refs/tan-form && mkdir -p refs/tan-form && curl -L https://github.com/TanStack/form/archive/refs/tags/@tanstack/react-form@1.27.6.tar.gz | tar -xz -C refs/tan-form --strip-components=1",
    "refs:base-ui": "rm -rf refs/base-ui && mkdir -p refs/base-ui && curl -L https://github.com/mui/base-ui/archive/refs/tags/v1.0.0.tar.gz | tar -xz -C refs/base-ui --strip-components=1",
    "refs:react-router": "rm -rf refs/react-router && mkdir -p refs/react-router && curl -L https://github.com/remix-run/react-router/archive/refs/tags/react-router@7.11.0.tar.gz | tar -xz -C refs/react-router --strip-components=1"
  },
  "dependencies": {
    "@base-ui/react": "1.0.0",
    "@better-auth/stripe": "1.3.32",
    "@cloudflare/actors": "0.0.1-beta.6",
    "@epic-web/invariant": "1.0.0",
    "@fontsource-variable/inter": "5.2.8",
    "@hookform/resolvers": "5.2.2",
    "@internationalized/date": "^3.10.0",
    "@tanstack/react-form": "1.27.6",
    "@tanstack/react-form-remix": "1.27.6",
    "aws4fetch": "1.0.20",
    "better-auth": "1.3.32",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "cmdk": "1.1.1",
    "date-fns": "4.1.0",
    "embla-carousel-react": "8.6.0",
    "hono": "4.10.4",
    "isbot": "5.1.28",
    "lucide-react": "0.514.0",
    "next-themes": "0.4.6",
    "react": "19.2.0",
    "react-day-picker": "9.13.0",
    "react-dom": "19.2.0",
    "react-resizable-panels": "3.0.6",
    "react-router": "7.11.0",
    "recharts": "2.15.4",
    "remix-themes": "2.0.4",
    "simple-icons": "15.16.1",
    "sonner": "2.0.7",
    "tailwind-merge": "3.3.1",
    "vaul": "1.1.2",
    "zod": "4.1.5"
  },
  "devDependencies": {
    "@better-auth/cli": "1.3.32",
    "@cloudflare/vite-plugin": "1.13.3",
    "@cloudflare/vitest-pool-workers": "0.9.3",
    "@eslint/js": "9.38.0",
    "@ianvs/prettier-plugin-sort-imports": "4.7.0",
    "@playwright/test": "1.56.1",
    "@react-router/dev": "7.11.0",
    "@react-router/fs-routes": "7.11.0",
    "@tailwindcss/vite": "4.1.16",
    "@types/node": "22.13.14",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "eslint": "9.38.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.0",
    "jsonc-parser": "3.3.1",
    "prettier": "3.6.2",
    "prettier-plugin-sql": "0.19.2",
    "prettier-plugin-tailwindcss": "0.7.1",
    "shadcn": "3.6.2",
    "stripe": "19.3.0",
    "tailwindcss": "4.1.16",
    "tw-animate-css": "1.4.0",
    "typescript": "5.9.2",
    "typescript-eslint": "8.46.2",
    "vite": "7.1.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.4",
    "wrangler": "4.38.0",
    "zx": "8.5.5"
  },
  "packageManager": "pnpm@10.4.0",
  "engines": {
    "node": "22.18.0"
  },
  "volta": {
    "node": "22.18.0",
    "pnpm": "10.4.0"
  }
}
